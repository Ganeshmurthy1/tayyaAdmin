package com.admin.knockoff.common.filter.page;

import java.math.BigDecimal;

public class KnockOffUtility {

	private long rowId;
	private String travelType;
	private BigDecimal totInvoiceAmount;
	private BigDecimal receiveAmount;
	public KnockOffUtility(long rowId, String travelType, BigDecimal totInvoiceAmount, BigDecimal receiveAmount) {
		super();
		this.setRowId(rowId);
		this.setTravelType(travelType);
		this.setTotInvoiceAmount(totInvoiceAmount);
		this.setReceiveAmount(receiveAmount);
	}
	public KnockOffUtility() {
		super();
	}
	public String getTravelType() {
		return travelType;
	}
	public void setTravelType(String travelType) {
		this.travelType = travelType;
	}
	public BigDecimal getTotInvoiceAmount() {
		return totInvoiceAmount;
	}
	public void setTotInvoiceAmount(BigDecimal totInvoiceAmount) {
		this.totInvoiceAmount = totInvoiceAmount;
	}
	public BigDecimal getReceiveAmount() {
		return receiveAmount;
	}
	public void setReceiveAmount(BigDecimal receiveAmount) {
		this.receiveAmount = receiveAmount;
	}
	public long getRowId() {
		return rowId;
	}
	public void setRowId(long rowId) {
		this.rowId = rowId;
	}

}
