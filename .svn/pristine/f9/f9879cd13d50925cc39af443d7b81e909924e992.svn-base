package com.lintas.utility;

import java.util.ArrayList;
import java.util.List;

public class BugTrackerUtility {
	private List<String> bugStatus=null;
	private List<String> bugType=null;
	private List<String> bugPriority=null;
	
	public BugTrackerUtility() {
		super();
		
		this.bugStatus = new ArrayList<>();
		this.bugStatus.add("Created");
		this.bugStatus.add("Pending");
		this.bugStatus.add("Assigned");
		this.bugStatus.add("WorkInProgress");
		this.bugStatus.add("StillInProgress");
		this.bugStatus.add("TestReview");
		this.bugStatus.add("Review");
		this.bugStatus.add("Closed");
	
		
		this.bugPriority= new ArrayList<>();
		this.bugPriority.add("Medium");
		this.bugPriority.add("Low");
		this.bugPriority.add("Critical");
		
		
		this.bugType= new ArrayList<>();
		this.bugType.add("NewFunctionality");
		this.bugType.add("Error");
		 }

	public List<String> getBugStatus() {
		return bugStatus;
	}

	public void setBugStatus(List<String> bugStatus) {
		this.bugStatus = bugStatus;
	}

	public List<String> getBugType() {
		return bugType;
	}

	public void setBugType(List<String> bugType) {
		this.bugType = bugType;
	}

	public List<String> getBugPriority() {
		return bugPriority;
	}

	public void setBugPriority(List<String> bugPriority) {
		this.bugPriority = bugPriority;
	}
	  
}
